@{
    ViewData["Title"] = "Tabs";
}

<div>
    <!-- Tab Button-->
    <ul id="tabstrip" class="nav nav-tabs" role="tablist">
        <li class="active">
            <a herf="#_FirstTab" role="tab" data-toggle="tab">Submission</a>
        </li>
        <li>
            <a herf="#_SecondTab" role="tab" data-toggle="tab">Search</a>
        </li>
        <li>
            <a herf="#_ThirdTab" role="tab" data-toggle="tab">Comments</a> 
        </li>

    </ul>

    <!--Tab Content Containers-->
    <div class="tab-content">
        <div class="tab-pane fade in active " id="_FirstTab">
            <partial name="Score" />
        </div>

        <div class="tab-pane fade in active " id="_SecondTab">
            <partial name="ChildList" />
        </div>

        <div class="tab-pane fade in active " id="_ThirdTab">
            <partial name="ChildViewData" view-data="ViewData" />
        </div>


    </div>
</div>


<partial name="ChildViewData" view-data="ViewData" />
<partial name="ChildList" />
<partial name="Score" />



